{
  "info": {
    "name": "Bookly - Seed Endpoints Collection",
    "description": "Comprehensive collection of seed endpoints organized by microservice",
    "version": "1.0.0",
    "_postman_id": "bookly-seed-endpoints-2024",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "auth": {
    "type": "bearer",
    "bearer": [
      {
        "key": "token",
        "value": "{{access_token}}",
        "type": "string"
      }
    ]
  },
  "variable": [
    {
      "key": "base_url",
      "value": "http://localhost:3000",
      "type": "string"
    },
    {
      "key": "auth_service_url",
      "value": "{{base_url}}/auth",
      "type": "string"
    },
    {
      "key": "access_token",
      "value": "",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "üèõÔ∏è Auth Service Seeds",
      "item": [
        {
          "name": "Check Seed Status",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{auth_service_url}}/seed/status",
              "host": ["{{auth_service_url}}"],
              "path": ["seed", "status"]
            },
            "description": "Check if database needs seeding"
          }
        },
        {
          "name": "Run Complete Seed",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "url": {
              "raw": "{{auth_service_url}}/seed/run",
              "host": ["{{auth_service_url}}"],
              "path": ["seed", "run"]
            },
            "description": "Execute complete seeding process"
          }
        }
      ],
