{
  "name": "bookly-backend",
  "version": "1.0.0",
  "description": "Bookly - Sistema de Reservas Institucionales Backend",
  "author": "Bookly Team",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "node --max-old-space-size=8192 node_modules/.bin/nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "node --max-old-space-size=4096 node_modules/.bin/nest start --watch",
    "start:dev:poll": "CHOKIDAR_USEPOLLING=true node --max-old-space-size=4096 node_modules/.bin/nest start --watch",
    "start:debug": "node --max-old-space-size=4096 node_modules/.bin/nest start --debug --watch",
    "start:debug:poll": "CHOKIDAR_USEPOLLING=true node --max-old-space-size=4096 node_modules/.bin/nest start --debug --watch",
    "start:prod": "node --max-old-space-size=2048 dist/main",
    "preconnect": "bash -lc 'source ~/.nvm/nvm.sh && nvm use v22.15.0'",
    "connect": "npm run connect:dev",
    "connect:dev": "npm run prisma:generate && npx concurrently -n GATEWAY,AUTH,RES,AVAIL,STOCK,REPORTS -c cyan,magenta,green,yellow,blue,orange \"npm:start:gateway\" \"npm:start:auth\" \"npm:start:resources\" \"npm:start:availability\" \"npm:start:stockpile\" \"npm:start:reports\"",
    "connect:debug": "npm run prisma:generate && npx concurrently -n GATEWAYD,AUTHD,RESD,AVAILD,STOCKD,REPORTSD -c cyanBright,magentaBright,greenBright,yellowBright,blueBright,orange \"npm:start:gateway:debug\" \"npm:start:auth:debug\" \"npm:start:resources:debug\" \"npm:start:availability:debug\" \"npm:start:stockpile:debug\" \"npm:start:reports:debug\"",
    "connect:prod": "NODE_ENV=production npx concurrently -n GATEWAY,AUTH,RES,AVAIL,STOCK,REPORTS -c cyan,magenta,green,yellow,blue,orange \"npm:start:gateway\" \"npm:start:auth\" \"npm:start:resources\" \"npm:start:availability\" \"npm:start:stockpile\" \"npm:start:reports\"",
    "start:gateway": "node --max-old-space-size=2048 -r ts-node/register -r tsconfig-paths/register src/apps/api-gateway/main.ts",
    "start:gateway:debug": "node --max-old-space-size=2048 --inspect=9229 -r ts-node/register -r tsconfig-paths/register src/apps/api-gateway/main.ts",
    "start:auth": "node --max-old-space-size=2048 -r ts-node/register -r tsconfig-paths/register src/apps/auth-service/main.ts",
    "start:auth:debug": "node --max-old-space-size=2048 --inspect=9230 -r ts-node/register -r tsconfig-paths/register src/apps/auth-service/main.ts",
    "start:resources": "node --max-old-space-size=2048 -r ts-node/register -r tsconfig-paths/register src/apps/resources-service/main.ts",
    "start:resources:debug": "node --max-old-space-size=2048 --inspect=9231 -r ts-node/register -r tsconfig-paths/register src/apps/resources-service/main.ts",
    "start:availability": "node --max-old-space-size=5120 -r ts-node/register -r tsconfig-paths/register src/apps/availability-service/main.ts",
    "start:availability:debug": "node --max-old-space-size=5120 --inspect=9232 -r ts-node/register -r tsconfig-paths/register src/apps/availability-service/main.ts",
    "start:stockpile": "node --max-old-space-size=2048 -r ts-node/register -r tsconfig-paths/register src/apps/stockpile-service/main.ts",
    "start:stockpile:debug": "node --max-old-space-size=2048 --inspect=9233 -r ts-node/register -r tsconfig-paths/register src/apps/stockpile-service/main.ts",
    "start:reports": "node --max-old-space-size=2048 -r ts-node/register -r tsconfig-paths/register src/apps/reports-service/main.ts",
    "start:reports:debug": "node --max-old-space-size=2048 --inspect=9234 -r ts-node/register -r tsconfig-paths/register src/apps/reports-service/main.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate",
    "prisma:db:push": "prisma db push",
    "prisma:db:seed": "node --max-old-space-size=2048 -r ts-node/register -r tsconfig-paths/register prisma/seed-simple.ts",
    "prisma:db:seed:full": "node --max-old-space-size=2048 -r ts-node/register -r tsconfig-paths/register prisma/seed.ts",
    "prisma:db:seed:clean": "prisma db push --force-reset && npm run prisma:db:seed",
    "prisma:db:seed:full:clean": "prisma db push --force-reset && npm run prisma:db:seed:full",
    "prisma:studio": "prisma studio",
    "docker:build": "docker build -t bookly-backend .",
    "docker:run": "docker run -p 3000:3000 bookly-backend"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/cqrs": "^10.2.6",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.3.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/platform-socket.io": "^10.3.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^5.1.1",
    "@nestjs/websockets": "^10.3.0",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/auto-instrumentations-node": "^0.41.0",
    "@opentelemetry/sdk-node": "^0.47.0",
    "@prisma/client": "^6.13.0",
    "@sentry/node": "^7.99.0",
    "@types/compression": "^1.8.1",
    "@types/passport-google-oauth20": "^2.0.16",
    "amqplib": "^0.10.3",
    "axios": "^1.11.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.8.1",
    "csv-parser": "^3.2.0",
    "googleapis": "^154.1.0",
    "helmet": "^8.1.0",
    "ical": "^0.8.0",
    "json2csv": "^6.0.0-alpha.2",
    "nestjs-i18n": "^10.4.5",
    "node-fetch": "^3.3.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma": "^6.13.0",
    "redis": "^4.6.12",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.5",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.0",
    "@types/amqplib": "^0.10.4",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "@types/passport-jwt": "^4.0.0",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^@libs/(.*)$": "<rootDir>/libs/$1",
      "^@apps/(.*)$": "<rootDir>/apps/$1",
      "^@common/(.*)$": "<rootDir>/libs/common/$1",
      "^@dto/(.*)$": "<rootDir>/libs/dto/$1",
      "^@event-bus/(.*)$": "<rootDir>/libs/event-bus/$1",
      "^@logging/(.*)$": "<rootDir>/libs/logging/$1",
      "^@monitoring/(.*)$": "<rootDir>/libs/monitoring/$1",
      "^@i18n/(.*)$": "<rootDir>/libs/i18n/$1"
    }
  }
}
