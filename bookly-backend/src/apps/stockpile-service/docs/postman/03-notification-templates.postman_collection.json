        {
          "name": "Update Notification Template",
          "request": {
            "method": "PUT",
            "header": [],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Enhanced Reservation Approved Email\",\n  \"description\": \"Enhanced email template with improved formatting\",\n  \"subject\": \"✅ Reservation Approved - {{resource.name}} on {{reservation.date}}\",\n  \"content\": {\n    \"html\": \"<div style='font-family: Arial, sans-serif; max-width: 600px;'><h2 style='color: #28a745;'>✅ Reservation Approved</h2><p>Dear {{user.firstName}},</p><p>Great news! Your reservation has been <strong style='color: #28a745;'>APPROVED</strong>.</p><div style='background: #f8f9fa; padding: 20px; border-radius: 8px; margin: 20px 0;'><h3 style='margin-top: 0;'>Reservation Details</h3><table style='width: 100%;'><tr><td><strong>Resource:</strong></td><td>{{resource.name}}</td></tr><tr><td><strong>Location:</strong></td><td>{{resource.location}}</td></tr><tr><td><strong>Date:</strong></td><td>{{reservation.date}}</td></tr><tr><td><strong>Time:</strong></td><td>{{reservation.startTime}} - {{reservation.endTime}}</td></tr><tr><td><strong>Purpose:</strong></td><td>{{reservation.purpose}}</td></tr></table></div><div style='background: #fff3cd; padding: 15px; border-radius: 8px; border-left: 4px solid #ffc107;'><h4 style='margin-top: 0; color: #856404;'>Important Reminders</h4><ul style='margin-bottom: 0;'><li>Please arrive on time</li><li>Keep the space clean after use</li><li>Report any issues immediately</li></ul></div><p style='margin-top: 20px;'>Approved by: <strong>{{approver.name}}</strong><br>{{approver.title}}</p><p style='color: #6c757d; font-size: 12px;'>This is an automated message from the Bookly Reservation System.</p></div>\",\n    \"text\": \"✅ RESERVATION APPROVED\\n\\nDear {{user.firstName}},\\n\\nGreat news! Your reservation has been APPROVED.\\n\\nReservation Details:\\n- Resource: {{resource.name}}\\n- Location: {{resource.location}}\\n- Date: {{reservation.date}}\\n- Time: {{reservation.startTime}} - {{reservation.endTime}}\\n- Purpose: {{reservation.purpose}}\\n\\nImportant Reminders:\\n- Please arrive on time\\n- Keep the space clean after use\\n- Report any issues immediately\\n\\nApproved by: {{approver.name}}\\n{{approver.title}}\\n\\nThis is an automated message from the Bookly Reservation System.\"\n  },\n  \"priority\": \"HIGH\",\n  \"isActive\": true\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
