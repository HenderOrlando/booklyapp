            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Updated Email Notifications\",\n  \"description\": \"Enhanced email notification channel with new configuration\",\n  \"configuration\": {\n    \"smtpHost\": \"smtp.ufps.edu.co\",\n    \"smtpPort\": 587,\n    \"username\": \"notifications@ufps.edu.co\",\n    \"fromName\": \"UFPS Bookly System\",\n    \"fromEmail\": \"bookly@ufps.edu.co\",\n    \"encryption\": \"TLS\",\n    \"enableBounceTracking\": true\n  },\n  \"isActive\": true,\n  \"priority\": 1\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{baseUrl}}/notification-templates/channels/{{testChannelId}}",
              "host": ["{{baseUrl}}"],
              "path": ["notification-templates", "channels", "{{testChannelId}}"]
            },
            "description": "Updates an existing notification channel (Admin only)"
          },
          "response": []
        }
      ]
    },
    {
      "name": "Notification Templates",
      "item": [
        {
          "name": "Create Notification Template",
          "request": {
            "method": "POST",
            "header": [],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Reservation Approved Email\",\n  \"description\": \"Email template for reservation approval notifications\",\n  \"channelId\": \"{{testChannelId}}\",\n  \"eventType\": \"RESERVATION_APPROVED\",\n  \"resourceType\": \"CLASSROOM\",\n  \"categoryId\": \"classroom-standard\",\n  \"subject\": \"Reservation Approved - {{resource.name}}\",\n  \"content\": {\n    \"html\": \"<h2>Reservation Approved</h2><p>Dear {{user.firstName}},</p><p>Your reservation for <strong>{{resource.name}}</strong> has been approved.</p><h3>Details:</h3><ul><li>Date: {{reservation.date}}</li><li>Time: {{reservation.startTime}} - {{reservation.endTime}}</li><li>Purpose: {{reservation.purpose}}</li></ul><p>Please arrive on time and follow facility guidelines.</p><p>Best regards,<br>{{approver.name}}</p>\",\n    \"text\": \"Reservation Approved\\n\\nDear {{user.firstName}},\\n\\nYour reservation for {{resource.name}} has been approved.\\n\\nDetails:\\n- Date: {{reservation.date}}\\n- Time: {{reservation.startTime}} - {{reservation.endTime}}\\n- Purpose: {{reservation.purpose}}\\n\\nPlease arrive on time and follow facility guidelines.\\n\\nBest regards,\\n{{approver.name}}\"\n  },\n  \"variables\": {\n    \"user\": [\"firstName\", \"lastName\", \"email\"],\n    \"resource\": [\"name\", \"location\"],\n    \"reservation\": [\"date\", \"startTime\", \"endTime\", \"purpose\"],\n    \"approver\": [\"name\", \"title\"]\n  },\n  \"isDefault\": true,\n  \"isActive\": true,\n  \"priority\": \"NORMAL\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{baseUrl}}/notification-templates",
              "host": ["{{baseUrl}}"],
              "path": ["notification-templates"]
            },
            "description": "Creates a new notification template"
          },
          "response": [],
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "pm.test('Status code is 201', function () {",
                  "  pm.response.to.have.status(201);",
                  "});",
                  "",
                  "pm.test('Template created successfully', function () {",
                  "  const responseJson = pm.response.json();",
                  "  pm.expect(responseJson).to.have.property('id');",
                  "  pm.expect(responseJson).to.have.property('name');",
                  "  pm.expect(responseJson).to.have.property('subject');",
                  "  pm.expect(responseJson).to.have.property('content');",
                  "});",
                  "",
                  "if (pm.response.code === 201) {",
                  "  const responseJson = pm.response.json();",
                  "  pm.environment.set('testTemplateId', responseJson.id);",
                  "}"
                ]
              }
            }
          ]
        },
