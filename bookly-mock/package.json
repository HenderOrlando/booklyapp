{
  "name": "@bookly/mock-monorepo",
  "version": "1.0.0",
  "description": "Bookly Mock - Full NestJS implementation with microservices",
  "private": true,
  "type": "commonjs",
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:all": "nest build api-gateway && nest build auth-service && nest build resources-service && nest build availability-service && nest build stockpile-service && nest build reports-service",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start api-gateway",
    "start:dev": "nest start api-gateway --watch",
    "start:debug": "nest start api-gateway --debug --watch",
    "start:prod": "node dist/apps/api-gateway/main",
    "start:gateway": "nest start api-gateway --watch",
    "start:gateway:debug": "nest start api-gateway --debug --watch",
    "start:auth": "nest start auth-service --watch",
    "start:auth:debug": "nest start auth-service --debug --watch",
    "start:resources": "nest start resources-service --watch",
    "start:resources:debug": "nest start resources-service --debug --watch",
    "start:availability": "nest start availability-service --watch",
    "start:availability:debug": "nest start availability-service --debug --watch",
    "start:stockpile": "nest start stockpile-service --watch",
    "start:stockpile:debug": "nest start stockpile-service --debug --watch",
    "start:reports": "nest start reports-service --watch",
    "start:reports:debug": "nest start reports-service --debug --watch",
    "start:all": "concurrently \"npm:start:gateway\" \"npm:start:auth\" \"npm:start:resources\" \"npm:start:availability\" \"npm:start:stockpile\" \"npm:start:reports\"",
    "start:all:debug": "concurrently \"npm:start:gateway:debug\" \"npm:start:auth:debug\" \"npm:start:resources:debug\" \"npm:start:availability:debug\" \"npm:start:stockpile\" \"npm:start:reports:debug\"",
    "lint": "eslint \"{apps,libs}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:bdd": "jest --testPathPattern=test/bdd",
    "test:auth": "jest --config apps/auth-service/jest.config.js",
    "test:resources": "jest --config apps/resources-service/jest.config.js",
    "test:availability": "jest --config apps/availability-service/jest.config.js",
    "test:stockpile": "jest --config apps/stockpile-service/jest.config.js",
    "test:reports": "jest --config apps/reports-service/jest.config.js",
    "test:auth:cov": "jest --config apps/auth-service/jest.config.js --coverage",
    "test:resources:cov": "jest --config apps/resources-service/jest.config.js --coverage",
    "test:availability:cov": "jest --config apps/availability-service/jest.config.js --coverage",
    "test:stockpile:cov": "jest --config apps/stockpile-service/jest.config.js --coverage",
    "test:reports:cov": "jest --config apps/reports-service/jest.config.js --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:up": "docker-compose up -d",
    "docker:up:microservices": "docker-compose -f docker-compose.microservices.yml up -d --build",
    "docker:down": "docker-compose down",
    "docker:down:microservices": "docker-compose -f docker-compose.microservices.yml down",
    "docker:logs": "docker-compose logs -f",
    "docker:logs:microservices": "docker-compose -f docker-compose.microservices.yml logs -f",
    "deploy:full": "powershell -ExecutionPolicy Bypass -File ../ci-cd/bookly-mock/scripts/docker/deploy.ps1 -Action start -Mode full",
    "deploy:partial": "powershell -ExecutionPolicy Bypass -File ../ci-cd/bookly-mock/scripts/docker/deploy.ps1 -Action start -Mode partial",
    "deploy:stop": "powershell -ExecutionPolicy Bypass -File ../ci-cd/bookly-mock/scripts/docker/deploy.ps1 -Action stop",
    "deploy:restart": "powershell -ExecutionPolicy Bypass -File ../ci-cd/bookly-mock/scripts/docker/deploy.ps1 -Action restart",
    "podman:full": "powershell -ExecutionPolicy Bypass -File ../ci-cd/bookly-mock/scripts/docker/podman.ps1 -Action start -Mode full",
    "podman:partial": "powershell -ExecutionPolicy Bypass -File ../ci-cd/bookly-mock/scripts/docker/podman.ps1 -Action start -Mode partial",
    "podman:stop": "powershell -ExecutionPolicy Bypass -File ../ci-cd/bookly-mock/scripts/docker/podman.ps1 -Action stop",
    "podman:restart": "powershell -ExecutionPolicy Bypass -File ../ci-cd/bookly-mock/scripts/docker/podman.ps1 -Action restart",
    "test:logger": "ts-node scripts/test-logger-colors.ts",
    "test:logger:colors": "ts-node scripts/test-logger-colors.ts",
    "seed:all": "npm run seed:auth && npm run seed:resources && npm run seed:availability && npm run seed:stockpile && npm run seed:reports",
    "seed:auth": "ts-node -r tsconfig-paths/register apps/auth-service/src/database/seed.ts",
    "seed:resources": "ts-node -r tsconfig-paths/register apps/resources-service/src/database/seed.ts",
    "seed:availability": "ts-node -r tsconfig-paths/register apps/availability-service/src/database/seed.ts",
    "seed:stockpile": "ts-node -r tsconfig-paths/register apps/stockpile-service/src/database/seed.ts",
    "seed:reports": "ts-node -r tsconfig-paths/register apps/reports-service/src/database/seed.ts",
    "validate:seeds": "npm run validate:seed:ids && npm run validate:seed:relations && npm run validate:seed:audit",
    "validate:seed:ids": "ts-node -r tsconfig-paths/register scripts/validate-seed-ids.ts",
    "validate:seed:relations": "ts-node -r tsconfig-paths/register scripts/validate-seed-relations.ts",
    "validate:seed:audit": "ts-node -r tsconfig-paths/register scripts/validate-seed-audit.ts",
    "script:fix-imports-syntax": "node scripts/fix-imports-syntax.js",
    "script:fix-imports-paths": "bash scripts/fix-imports-paths.sh",
    "script:fix-imports": "ts-node scripts/fix-imports.ts",
    "script:fix-tsconfig": "bash scripts/fix-tsconfig-rootdir.sh",
    "script:start-all": "bash scripts/start-all-services.sh",
    "script:start-prod": "bash scripts/start-all-prod.sh",
    "script:test-websocket": "ts-node scripts/test-websocket-client.ts",
    "script:seed-events": "ts-node scripts/seed-events-for-replay.ts",
    "script:test-pagination": "ts-node -r tsconfig-paths/register scripts/check-pagination.ts",
    "script:verify-dlq": "bash scripts/verify-dlq-fix.sh",
    "script:verify-eventbus": "bash scripts/verify-event-bus-integration.sh",
    "script:test-eventbus": "bash scripts/test-event-bus.sh",
    "script:test-event-replay": "bash scripts/test-event-replay.sh",
    "script:start-brokers": "bash scripts/start-event-brokers.sh"
  },
  "dependencies": {
    "@azure/msal-node": "^3.8.1",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/cqrs": "^10.2.6",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.3.0",
    "@nestjs/mongoose": "^10.0.2",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/platform-socket.io": "^10.4.20",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/websockets": "^10.4.20",
    "@types/bcryptjs": "^2.4.6",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/pdfkit": "^0.17.3",
    "@types/qrcode": "^1.5.6",
    "@types/speakeasy": "^2.0.10",
    "amqplib": "^0.10.3",
    "axios": "^1.13.1",
    "bcryptjs": "^3.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "date-fns": "^3.0.6",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.5",
    "force": "^0.0.3",
    "googleapis": "^144.0.0",
    "helmet": "^7.1.0",
    "kafkajs": "^2.2.4",
    "mongoose": "^8.0.3",
    "node-fetch": "^2.7.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.17.2",
    "qrcode": "^1.5.4",
    "redis": "^4.6.12",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "speakeasy": "^2.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.3.0",
    "@types/amqplib": "^0.10.1",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.25",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/passport-jwt": "^4.0.0",
    "@types/passport-local": "^1.0.38",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "glob": "^13.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "socket.io-client": "^4.8.1",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@libs/(.*)$": "<rootDir>/libs/$1",
      "^@auth/(.*)$": "<rootDir>/apps/auth-service/src/$1",
      "^@resources/(.*)$": "<rootDir>/apps/resources-service/src/$1",
      "^@availability/(.*)$": "<rootDir>/apps/availability-service/src/$1",
      "^@stockpile/(.*)$": "<rootDir>/apps/stockpile-service/src/$1",
      "^@reports/(.*)$": "<rootDir>/apps/reports-service/src/$1",
      "^@gateway/(.*)$": "<rootDir>/apps/api-gateway/src/$1"
    }
  }
}
